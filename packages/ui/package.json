{
  "name": "@craftedtales/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "types": "types/index.d.ts",
  "files": [
    "types",
    "src",
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.mjs"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "build": "tamagui-build",
    "watch": "tamagui-build --watch"
  },
  "devDependencies": {
    "@craftedtales/config": "workspace:*",
    "eslint": "^9.39.1",
    "@tamagui/build": "^1.138.0",
    "react": "^19.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "@tamagui/colors": "^1.138.0",
    "@tamagui/config": "^1.138.0",
    "@tamagui/core": "^1.138.0",
    "@tamagui/theme-builder": "^1.138.0",
    "tamagui": "^1.138.0"
  }
}
{
	"servers": {
		"chakra-ui": {
			"command": "npx",
			"args": [
				"-y",
				"@chakra-ui/react-mcp"
			],
			"type": "stdio"
		},
		"i18next documentation": {
			"url": "https://www.i18next.com/~gitbook/mcp",
			"type": "http"
		},
		"cloudflare": {
			"command": "npx",
			"args": [
				"mcp-remote",
				"https://docs.mcp.cloudflare.com/mcp"
			]
		},
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp"
			]
		}
	},
	"inputs": []
}